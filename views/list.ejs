<%- include("header") -%>

<% switches.forEach((switcher) => { %>
    <p><%= switcher.name %></p>
    <%console.log(switcher.name)%>
    <form action="/on" method="post">
        <button type="submit" name="switcherIdentifier" value=<%=switcher.identifier%>>On</button>
    </form>
    <form action="/off" method="post">
        <button type="submit" name="switcherIdentifier" value=<%=switcher.identifier%>>Off</button>
    </form>
<% }); %> 


<%- include("footer") -%>